                                id="model-phone" class="form-control with-icon" placeholder="">
                            <i class="rt-icon2-phone5"></i>
                        </div>
                        <div class="form-group">
                            <label for="model-message" class="sr-only">Message</label>
                            <textarea aria-required="true" rows="6" cols="45" name="model-message"
                                id="model-message" class="form-control with-icon" placeholder=""></textarea>
                            <i class="rt-icon2-pen"></i>
                        </div>

                        <button type="submit" id="model_contact_form_submit" name="model_contact_submit"
